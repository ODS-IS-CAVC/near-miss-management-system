{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build:dev": "cp .env.dev .env && npm run openapi:fetch && npm run orval && ng build",
    "build:stg": "cp .env.stg .env && npm run openapi:fetch && npm run orval && ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier": "prettier --write './**/*.{js,css,html,json,ts,tsx,jsx}'",
    "format": "npm run prettier && npm run lint",
    "openapi:fetch": "curl http://localhost:3000/openapi-yaml -o ./swagger/openapi.yml",
    "orval": "npm run openapi:fetch && orval --config ./orval.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "axios": "^1.7.4",
    "bytes": "^3.1.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "nanoid": "^5.0.7",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.3",
    "@angular/cli": "^18.1.3",
    "@angular/compiler-cli": "^18.1.0",
    "@types/bytes": "^3.1.4",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "18.3.0",
    "eslint": "^9.9.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "orval": "^7.0.1",
    "puppeteer": "^23.4.1",
    "typedoc": "^0.26.7",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.1.0"
  }
}
